= render :partial => 'shared/header_vote'
.ninety
  %table.full.item
    %tr.row0
      %th{:colspan => 2}
        = @question
    %tr
      %td= t('items.idea')
      %td= @item
    %tr
      %td= t('items.added_on')
      %td= @added
    %tr
      %td= t('items.score')
      %td= @score.to_i
    %tr
      %td= t('items.number_of_votes')
      %td= @wins.to_i + @losses.to_i